{
  "name": "server",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon",
    "build": "cd ../client && npm install && npm run build",
    "start": "node production-server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=unit",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "seed:test-users": "ts-node src/scripts/seed-test-users.ts",
    "agent:schema": "ts-node src/agents/database-schema/cli.ts",
    "agent:schema:validate": "ts-node src/agents/database-schema/cli.ts validate",
    "agent:schema:migrate": "ts-node src/agents/database-schema/cli.ts migrate",
    "agent:schema:optimize": "ts-node src/agents/database-schema/cli.ts optimize",
    "agent:schema:docs": "ts-node src/agents/database-schema/cli.ts docs",
    "agent:schema:backup": "ts-node src/agents/database-schema/cli.ts backup",
    "agent:schema:restore": "ts-node src/agents/database-schema/cli.ts restore",
    "agent:schema:seed": "ts-node src/agents/database-schema/cli.ts seed",
    "agent:schema:health": "ts-node src/agents/database-schema/cli.ts health",
    "agent:api": "ts-node src/agents/api-development/cli.ts",
    "agent:api:generate": "ts-node src/agents/api-development/cli.ts generate",
    "agent:api:analyze": "ts-node src/agents/api-development/cli.ts analyze",
    "agent:api:docs": "ts-node src/agents/api-development/cli.ts docs",
    "agent:api:health": "ts-node src/agents/api-development/cli.ts health",
    "setup:api": "ts-node src/setup-api.ts",
    "api:dev": "nodemon --exec ts-node src/setup-api.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "@prisma/client": "^6.14.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/nodemailer": "^7.0.1",
    "axios": "^1.11.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "nodemailer": "^7.0.5"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.33.0",
    "jest": "^30.0.5",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "prisma": "^6.14.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
