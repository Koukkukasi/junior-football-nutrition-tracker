{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:headed": "playwright test --headed",
    "test:food-log": "playwright test tests/e2e/food-log.spec.ts",
    "test:report": "playwright show-report",
    "test:codegen": "playwright codegen",
    "test:trace": "playwright show-trace",
    "test:install": "playwright install",
    "test:agent": "node -r ts-node/register agents/testing-agent/runner.ts",
    "test:visual": "playwright test --grep visual",
    "test:mobile": "playwright test --project=mobile-chrome --project=mobile-safari",
    "test:desktop": "playwright test --project=chromium --project=firefox --project=webkit"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.43.0",
    "@tanstack/react-query": "^5.85.5",
    "axios": "^1.11.0",
    "lucide-react": "^0.540.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@playwright/test": "^1.40.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/node": "^20.10.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12",
    "terser": "^5.43.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}
